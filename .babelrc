{
    "presets": [
        "@babel/env",
        "@babel/react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "prismjs",
            {
                "languages": [
                    "javascript",
                    "css",
                    "html"
                ],
                "plugins": [
                    "line-numbers",
                    "show-language"
                ],
                "theme": "okaidia",
                "css": true
            }
        ]
    ]
}